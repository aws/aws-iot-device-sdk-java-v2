{
    "language": "Java",
    "runnable_file": "samples/WindowsCertConnect",
    "runnable_region": "us-east-1",
    "runnable_main_class": "windowscertconnect.WindowsCertConnect",
    "arguments": [
        {
            "name": "--endpoint",
            "secret": "ci/endpoint"
        },
        {
            "name": "--cert",
            "windows_cert_certificate": "ci/PubSub/cert",
            "windows_cert_certificate_path": "tmp_cert.pem",
            "windows_cert_key": "ci/PubSub/key",
            "windows_cert_key_path": "tmp_key.pem",
            "windows_cert_pfx_key_path": "tmp_pfx.pem"
        }
    ]
}
