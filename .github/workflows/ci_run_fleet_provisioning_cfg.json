{
    "language": "Java",
    "sample_file": "samples/FleetProvisioning",
    "sample_region": "us-east-1",
    "sample_main_class": "fleetprovisioning.FleetProvisioningSample",
    "arguments": [
        {
            "name": "--endpoint",
            "secret": "ci/endpoint"
        },
        {
            "name": "--cert",
            "secret": "ci/FleetProvisioning/cert",
            "filename": "tmp_certificate.pem"
        },
        {
            "name": "--key",
            "secret": "ci/FleetProvisioning/key",
            "filename": "tmp_key.pem"
        },
        {
            "name": "--template_name",
            "data": "CI_FleetProvisioning_Template"
        },
        {
            "name": "--template_parameters",
            "data": "{SerialNumber:$INPUT_UUID}"
        }
    ]
}
