{
    "name": "aws-iot-device-sdk-java-v2",
    "packages": [
        "maven"
    ],
    "build_steps": [
        "mvn -B compile"
    ],
    "test_steps": [
        "aws s3 cp s3://iot-sdk-ci-bucket-us-east1/IotUsProdMqtt5EnvironmentVariables.txt ./environment_files.txt",
        "source utils/mqtt5_test_setup.sh s3://iot-sdk-ci-bucket-us-east1/IotUsProdMqtt5EnvironmentVariables.txt readonly",
        "mvn test -Dtest=Mqtt5BuilderTest -DfailIfNoTests=false",
        "source utils/mqtt5_test_setup.sh s3://iot-sdk-ci-bucket-us-east1/IotUsProdMqtt5EnvironmentVariables.txt cleanup"
    ],
    "imports": [
        "JDK8"
    ],
    "env": {
    },
    "hosts": {
        "ubuntu": {
            "packages": [
                "openjdk-8-jdk-headless"
            ]
        },
        "debian": {
            "packages": [
                "openjdk-8-jdk-headless"
            ]
        }
    },
    "variants" : {
        "skip_test": {
            "!test_steps": []
        }
      }
}
