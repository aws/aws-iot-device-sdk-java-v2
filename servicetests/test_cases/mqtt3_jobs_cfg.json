{
    "language": "Java",
    "runnable_file": "tests/JobsExecution",
    "runnable_region": "us-east-1",
    "runnable_main_class": "jobsExecution.JobsExecution",
    "arguments": [
        {
            "name": "--mqtt_version",
            "data": 3
        },
        {
            "name": "--endpoint",
            "secret": "ci/endpoint"
        },
        {
            "name": "--cert",
            "data": "certificate.pem.crt"
        },
        {
            "name": "--key",
            "data": "private.pem.key"
        },
        {
            "name": "--thing_name",
            "data": "ServiceTest_Jobs_$INPUT_UUID"
        }
    ]
}
